import random as rm
digits = '0123456789'
uppercase_letters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'
lowercase_letters = 'abcdefghijklmnopqrstuvwxyz'
punctuation = '!#$%&*+-=?@^_'
chars = ''

quality = int(input('Укажите количество паролей для генерации '))
lenth = int(input('Укажите длину одного пароля '))
q1 = input('Включать ли цифры 0123456789? (да/нет) ')
q2 = input('Включать ли прописные буквы ABCDEFGHIJKLMNOPQRSTUVWXYZ? (да/нет) ')
q3 = input('Включать ли строчные буквы abcdefghijklmnopqrstuvwxyz? (да/нет) ')
q4 = input('Включать ли символы !#$%&*+-=?@^_? (да/нет) ')
q5 = input('Исключать ли неоднозначные символы il1Lo0O? (да/нет) ')

if q1.lower() == 'да':
    chars += digits 
if q2.lower() == 'да':
    chars += uppercase_letters
if q3.lower() == 'да':
    chars += lowercase_letters
if q4.lower() == 'да':
    chars += punctuation
if q5.lower() == 'да':
    for c in 'il1Lo0O':
        chars.replace(c,'')
print()    
#print(chars)

def generate_password(lenth, chars):
    password = ''
    for i in range(lenth):
        password += rm.choice(chars)
    print(password)

for i in range(quality):
    generate_password(lenth, chars)
