a, b = input(), input()
a = a.lower()
b = b.lower()

new_a = ''
for i in range(len(a)):
    if 97 <= ord(a[i]) <= 122 or 1072 <= ord(a[i]) <= 1103:
        new_a = new_a + a[i]
#print(new_a)

new_b = ''
for i in range(len(b)):
    if 97 <= ord(b[i]) <= 122 or 1072 <= ord(b[i]) <= 1103:
        new_b = new_b + b[i]
#print(new_b)

flag = True
for i in range(len(new_a)):
    if new_a[i] != new_b[i]:
        flag = False
        break

if flag == True:
    print('YES')
else:
    print('NO')
